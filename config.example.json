{
    "mcpServers": {
        "web_search": {
            "command": "python",
            "args": [
                "./mcp_server/web_search/server.py"
            ],
            "env": {
                "ZHIPU_API_KEY": "",
                "SERPER_API_KEY": ""
            },
            "description": "Web Search tools to get information on the web"
        },
        "python-exec": {
            "command": "python",
            "args": [
                "./mcp_server/python_executor/server.py"
            ]
        },
        "scholar_search": {
            "command": "python",
            "args": [
                "./mcp_server/scholar_search/server.py"
            ]
        },
        "bilibili-search": {
            "command": "python",
            "args": [
                "./mcp_server/bilibili_search/server.py"
            ],
            "env": {
                "sessdata": "",
                "bili_jct": "",
                "buvid3": ""
            }
        },
        "12306-mcp": {
            "command": "node",
            "args": [
                "./mcp_server/train_search/build/index.js"
            ]
        },
        "wechat_search": {
            "command": "python",
            "args": [
                "./mcp_server/wechat_search/server.py"
            ]
        },
        "douban_search": {
            "command": "node",
            "args": [
                "./mcp_server/douban_search/dist/index.js"
            ],
            "env": {
                "COOKIE": ""
            }
        },
        "amap-mcp-server": {
            "command": "python",
            "args": [
                "./mcp_server/amap_mcp_server/server.py"
            ],
            "env": {
                "AMAP_MAPS_API_KEY": ""
            }
        },
        "local_SAI-database": {
            "command": "python",
            "args": [
                "-u",
                "./mcp_server/local_sai_search/server.py"
            ]
        }
    }
}